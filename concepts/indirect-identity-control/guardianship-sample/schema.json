[
  "@context", //JSON array where first = uri w3.org/2018/credentials/v1
  "id", // optional
  "type", //"[\",VerifiableCredential\", \"Guardaniship/1.0\"]",
  "credentialSchema", // points to cred def
  "issuanceDate", // standard in VCs RFC3339 Date-Time string
  "expirationDate",
  "credentialStatus", { "id" /*points to rev reg*/, "type": "sovrin rev reg 1.0" }
  "issuer", // <did>
  "trust_framework", // uri to a website
  "audit_uri", // constrained by trust framework, but a URI
  "appeal_uri", // same
  "credentialSubject.holder.type", // uri to schema for a holder
  "credentialSubject.holder.role", // "parent" or "grandparent"
  "credentialSubject.holder.basis_uri", // uri from trust framework that defines which basis justifies the guardianship
  "credentialSubject.holder.first_name",
  "credentialSubject.holder.limits.boundaries",
  "credentialSubject.holder.limits.point_of_origin",
  "credentialSubject.holder.limits.radius_km",
  "credentialSubject.holder.limits.jurisdictions",
  "credentialSubject.holder.limits.trigger",
  "credentialSubject.holder.limits.circumstances",
  "credentialSubject.holder.limits.start_time",
  "credentialSubject.holder.limits.end_time",
  "credentialSubject.dependent.first_name",
  "credentialSubject.dependent.permissions", // string of JSON array of grants
  "credentialSubject.dependent.last_name",
  "credentialSubject.dependent.birth_date",
  "credentialSubject.dependent.gender",
  "credentialSubject.dependent.native_language",
  "credentialSubject.dependent.identifying_marks",
  "credentialSubject.dependent.photo",
  "credentialSubject.dependent.iris",
  "credentialSubject.dependent.fingerprint",
]
